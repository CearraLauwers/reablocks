{
  "name": "reablocks",
  "version": "4.3.4",
  "description": "Component library for React",
  "scripts": {
    "build": "vite build --mode library",
    "build-storybook": "storybook build",
    "copy": "cp CNAME storybook-static/",
    "lint": "eslint --ext js,ts,tsx",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "lint:prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "storybook dev -p 9009",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/reablocks.umd.cjs",
  "module": "./dist/reablocks.js",
  "source": "src/index.ts",
  "browser": "dist/index.js",
  "style": "dist/index.css",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaviz/reablocks.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "tree"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/reaviz/reablocks/issues"
  },
  "homepage": "https://github.com/reaviz/reablocks#readme",
  "dependencies": {
    "@reaviz/react-use-fuzzy": "^1.0.3",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.1",
    "coverup": "^0.1.1",
    "create-global-state-hook": "^0.0.2",
    "date-fns": "^2.30.0",
    "ellipsize": "^0.5.1",
    "focus-trap-react": "^8.7.1",
    "framer-motion": "7.8.0",
    "fuse.js": "^6.6.2",
    "human-format": "^1.2.0",
    "name-initials": "^0.1.3",
    "pluralize": "^8.0.0",
    "rdk": ">=6.4.4",
    "react-fast-compare": "^3.2.2",
    "react-highlight-words": "^0.17.0",
    "react-input-autosize": "^3.0.0",
    "string-to-color": "^2.2.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^7.0.18",
    "@storybook/addon-essentials": "7.0.18",
    "@storybook/addon-mdx-gfm": "7.0.18",
    "@storybook/addon-storysource": "^7.0.18",
    "@storybook/addons": "7.0.18",
    "@storybook/react": "7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/theming": "7.0.18",
    "@types/classnames": "^2.3.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.1",
    "autoprefixer": "^9",
    "chromatic": "^6.17.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^4.2.5",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.1.2",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.4.1",
    "prettier": "^2.8.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.43.9",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "storybook": "7.0.18",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-css-injected-by-js": "^3.1.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
