{
  "name": "reablocks",
  "version": "3.6.6",
  "description": "Component library for React",
  "scripts": {
    "build": "rollup -c",
    "prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 9009",
    "build-storybook": "export NODE_OPTIONS=--openssl-legacy-provider && build-storybook",
    "lint": "eslint --ext js,ts,tsx",
    "lint:fix": "eslint --ext js,ts,tsx --fix src",
    "copy": "cp CNAME storybook-static/",
    "lint:prettier": "prettier --loglevel warn --write 'src/**/*.{ts,tsx,js,jsx}'"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "style": "dist/index.css",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaviz/reablocks.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "tree"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/reaviz/reablocks/issues"
  },
  "homepage": "https://github.com/reaviz/reablocks#readme",
  "dependencies": {
    "@reaviz/react-use-fuzzy": "^1.0.2",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.1",
    "coverup": "^0.1.1",
    "create-global-state-hook": "^0.0.2",
    "date-fns": "^2.29.3",
    "ellipsize": "^0.5.1",
    "focus-trap-react": "^8.7.1",
    "framer-motion": "7.8.0",
    "fuse.js": "^6.6.2",
    "human-format": "^1.0.0",
    "pluralize": "^8.0.0",
    "rdk": "^6.4.1",
    "react-fast-compare": "^3.2.0",
    "react-highlight-words": "^0.17.0",
    "react-input-autosize": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/theming": "6.5.16",
    "@types/classnames": "^2.3.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^9",
    "chromatic": "^6.17.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^4.2.5",
    "lint-staged": "^13.1.2",
    "postcss-nested": "^4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.8.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.43.1",
    "rollup": "^3.15.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "3.1.3",
    "rollup-plugin-postcss-modules": "2.0.2",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-typescript2": "0.34.1",
    "storybook-css-modules-preset": "^1.1.1",
    "storybook-dark-mode": "^2.0.6",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
